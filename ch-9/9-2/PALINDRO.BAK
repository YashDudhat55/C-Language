#include<conio.h>
#include<stdio.h>
#define pf printf

	int a, b = 0, c, n;

int main(){

	clrscr();

	pf("Enter Number : ");
	scanf("%d",&a);

	n = a;

	while(a > 0){

		c = a % 10;
		b = b * 10 + c;
		a = a / 10;
	}

